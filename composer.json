{
    "name": "baijunyao/laravel-support",
    "keywords": ["laravel", "support"],
    "description": "Laravel Support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "baijunyao",
            "email": "baijunyao@baijunyao.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.5|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Baijunyao\\LaravelSupport\\": "src/",
            "Baijunyao\\LaravelSupport\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/larastan": "^0.4.3",
        "orchestra/testbench": "^4.3",
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.4"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "phpunit": "vendor/bin/phpunit -c phpunit.xml"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baijunyao\\LaravelSupport\\SupportServiceProvider"
            ]
        }
    }
}
